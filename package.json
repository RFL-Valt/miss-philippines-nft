{
  "name": "kog-core",
  "version": "1.0.0",
  "description": "Core ERC1155 for Kogs Collectible",
  "main": "index.js",
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "deploy:testnet": "npx hardhat deploy --network testnet --reset",
    "deploy:local": "npx hardhat deploy --reset",
    "deploy:mainnet": "npx hardhat deploy --network mainnet",
    "verify:testnet": "npx hardhat etherscan-verify --network testnet",
    "verify:mainnet": "npx hardhat etherscan-verify --network mainnet",
    "lint": "npx prettier --list-different contracts/*.sol",
    "prettier": "npx prettier --write contracts/*.sol"
  },
  "keywords": [
    "kog",
    "erc1155"
  ],
  "author": "RFOX Labs",
  "license": "MIT",
  "dependencies": {
    "@chainlink/contracts": "^0.2.0",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.3.1",
    "@openzeppelin/hardhat-upgrades": "^1.9.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "hardhat": "^2.4.1",
    "hardhat-abi-exporter": "^2.2.1",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.8.9",
    "hardhat-gas-reporter": "^1.0.4",
    "multi-token-standard": "^2.2.2"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.0",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.14",
    "solidity-coverage": "^0.7.16"
  }
}
